command[check_users]=/usr/lib/nagios/plugins/check_users -w 5 -c 10
command[check_load]=/usr/lib/nagios/plugins/check_load -w <%= @processorcount.to_i * 3 %>,<%= @processorcount.to_i * 2 %>,<%= @processorcount.to_i %> -c <%= @processorcount.to_i * 6 %>,<%= @processorcount.to_i * 4 %>,<%= @processorcount.to_i * 2 %>
command[check_total_procs]=/usr/lib/nagios/plugins/check_procs -w 400 -c 500
command[check_all_disks]=/usr/lib/nagios/plugins/check_disk -w 20% -c 10%
command[check_puppetagent]=sudo /usr/lib/nagios/plugins/check_puppetagent -w 1860 -c 2700
command[check_ceph_health]=/usr/lib/nagios/plugins/check_ceph_health
command[check_ceph_mon]=sudo /usr/lib/nagios/plugins/check_ceph_mon -H <%= @ipaddress_p1p1 %> -I <%= @hostname %>
command[check_ceph_osd]=sudo /usr/lib/nagios/plugins/check_ceph_osd -H <%= @ipaddress_p1p1 %>
command[check_ceph_rgw]=sudo /usr/lib/nagios/plugins/check_ceph_rgw -i radosgw.<%= @hostname %>
