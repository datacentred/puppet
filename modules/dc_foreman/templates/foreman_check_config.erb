[foreman]
foreman_api_user = <%= @foreman_view_api_user %>
foreman_api_pw = <%= @foreman_view_api_pw %>
foreman_api_baseurl = https://<%= @foreman_url %>/api/
cert_path = <%= @_cert %>
cacert_path = <%= @_cacert %>
key_path = <%= @_key %>
proxy_alias = <%= @proxy_alias %>

[omapi]
omapi_port = <%= @omapi_port %>
omapi_key = <%= @omapi_key %>
omapi_secret = <%= @omapi_secret %>
dhcp_server = <%= @dhcp_server %>

[pxe]
pxe_root = <%= @tftp_dir %>/pxelinux.cfg

[dhcp]
lease_file = <%= @lease_file %>

[dns]
key = <%= @dns_key %>
