    {
      "paths": [ "<%= @logs %>" ],
      "fields": <%= @json_fields %>,
      <%- if @codec_hash -%>
          <%- @codec_hash.each_pair do |hash_key,hash_val| -%>
      "codec": {
            <%- hash_val.each_pair do |key,val| -%>
                <%- if val == 'true' or val == 'false' -%>
            "<%= key %>": <%= val %>,
                <%- else -%>
            "<%= key %>": "<%= val %>",
                <%- end -%>
            <%- end -%>
      }
          <%- end -%>
      <%- end -%>
    },
