<Host "<%= @title %>">
  Address "<%= @ip %>"
  Version 3
  Username "<%= @snmp_username %>"
  AuthProtocol "<%= @snmp_auth_protocol %>"
  AuthPassphrase "<%= @snmp_auth_password %>"
  PrivacyProtocol "<%= @snmp_privacy_protocol %>"
  PrivacyPassphrase "<%= @snmp_privacy_password %>"
  SecurityLevel "authPriv"
  Collect "traffic"
  Interval <%= @interval %>
</Host>
