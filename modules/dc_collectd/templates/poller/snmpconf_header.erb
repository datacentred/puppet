LoadPlugin snmp
<Plugin snmp>
  <Data "snmp_if_octets">
       Type "if_octets"
       Table true
       Instance "IF-MIB::ifName"
       Values "IF-MIB::ifInOctets" "IF-MIB::ifOutOctets"
   </Data>
   <Data "snmp_if_errors">
       Type "if_errors"
       Table true
       Instance "IF-MIB::ifName"
       Values "IF-MIB::ifInErrors" "IF-MIB::ifOutErrors"
   </Data>
   <Data "snmp_if_dropped">
       Type "if_dropped"
       Table true
       Instance "IF-MIB::ifName"
       Values "IF-MIB::ifInDiscards" "IF-MIB::ifOutDiscards"
   </Data>
   <Data "snmp_if_packets">
       Type "if_packets"
       Table true
       Instance "IF-MIB::ifName"
       Values "IF-MIB::ifInUcastPkts" "IF-MIB::ifOutUcastPkts"
   </Data>
